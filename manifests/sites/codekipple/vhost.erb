# File Managed by Puppet

<VirtualHost *:80>
    ServerAdmin webmaster@codekipple.dev.codekipple.com
    DocumentRoot /var/www/codekipple.github.io
    ServerName codekipple.dev.codekipple.com

    ErrorLog  /var/log/apache2/codekipple.dev.codekipple.com-error_log
    CustomLog /var/log/apache2/codekipple.dev.codekipple.com-access_log common

    <Directory /var/www/missbolt/web>
        AllowOverride All
    </Directory>

</VirtualHost>