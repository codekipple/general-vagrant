# File Managed by Puppet

<VirtualHost *:80>
    ServerAdmin webmaster@missbolt.dev.codekipple.com
    DocumentRoot /var/www/missbolt/web
    ServerName missbolt.dev.codekipple.com

    ErrorLog  /var/log/apache2/missbolt.dev.codekipple.com-error_log
    CustomLog /var/log/apache2/missbolt.dev.codekipple.com-access_log common

    <Directory /var/www/missbolt/web>
        AllowOverride All
    </Directory>

</VirtualHost>